@prefix paizo: <http://paizo.com/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# =============================================================================
# БАЗОВАЯ ОНТОЛОГИЯ PAIZO
# =============================================================================

<http://paizo.com/ontology> rdf:type owl:Ontology ;
    rdfs:label "Базовая онтология Paizo"@ru,
               "Paizo Base Ontology"@en ;
    rdfs:comment "Основная структура онтологии для описания приключений Paizo"@ru,
                 "Base ontology structure for describing Paizo adventures"@en .

# =============================================================================
# ОПРЕДЕЛЕНИЕ КЛАССОВ
# =============================================================================

# Классы для персонажей и актеров

paizo:Agent rdf:type owl:Class ;
    rdfs:label "Агент"@ru,
                 "Agent"@en ;
    rdfs:comment "Действующее лицо или сущность, способная совершать действия или участвовать в событиях."@ru,
                 "Acting entity capable of performing actions or participating in events."@en .

paizo:Character rdf:type owl:Class ;
    rdfs:subClassOf paizo:Agent ;
    rdfs:label "Персонаж"@ru,
                 "Character"@en ;
    rdfs:comment "Персонаж (субъект), который является актором или участником событий приключения."@ru,
                 "Character (subject) that is an actor or participant in adventure events."@en .

paizo:CreatureType rdf:type owl:Class ;
    rdfs:subClassOf paizo:Agent ;
    rdfs:label "Тип существа"@ru,
                 "Creature Type"@en ;
    rdfs:comment "Абстрактная категория существ, представляющая вид или тип, а не конкретную особь."@ru,
                 "Abstract category of creatures representing a species or type, not a specific individual."@en .

paizo:PlayerCharacter rdf:type owl:Class ;
    rdfs:subClassOf paizo:Character ;
    rdfs:label "Игровой персонаж"@ru,
                 "Player Character"@en ;
    rdfs:comment "Персонаж, управляемый игроком."@ru,
                 "Character controlled by a player."@en .

paizo:NonPlayerCharacter rdf:type owl:Class ;
    rdfs:subClassOf paizo:Character ;
    rdfs:label "Неигровой персонаж"@ru,
                 "Non-Player Character"@en ;
    rdfs:comment "Персонаж, управляемый мастером игры. Обладает ролью по отношению к персонажам игроков."@ru,
                 "Character controlled by the game master. Has a role towards player characters."@en .

paizo:Group rdf:type owl:Class ;
    rdfs:subClassOf paizo:Agent ;
    rdfs:label "Группа персонажей"@ru,
                 "Character Group"@en ;
    rdfs:comment "Коллектив или группа персонажей, действующих как единое целое."@ru,
                 "Collective or group of characters acting as a single entity."@en .

paizo:AuthorityFigure rdf:type owl:Class ;
    rdfs:subClassOf paizo:NonPlayerCharacter ;
    rdfs:label "Представитель власти"@ru,
                 "Authority Figure"@en ;
    rdfs:comment "Персонаж, занимающий официальную должность."@ru,
                 "Character holding an official position."@en .

paizo:Merchant rdf:type owl:Class ;
    rdfs:subClassOf paizo:NonPlayerCharacter ;
    rdfs:label "Торговец"@ru,
                 "Merchant"@en ;
    rdfs:comment "Персонаж, занимающийся торговлей."@ru,
                 "Character engaged in trade."@en .

paizo:ReligiousFigure rdf:type owl:Class ;
    rdfs:subClassOf paizo:NonPlayerCharacter ;
    rdfs:label "Религиозный деятель"@ru,
                 "Religious Figure"@en ;
    rdfs:comment "Персонаж, служащий божеству или религиозной организации."@ru,
                 "Character serving a deity or religious organization."@en .


# Классы для событий
paizo:Event rdf:type owl:Class ;
    rdfs:label "Событие"@ru,
                 "Event"@en ;
    rdfs:comment "Изменения (предикат) в игровом мире."@ru,
                 "Changes (predicate) in the game world."@en .

paizo:Adventure rdf:type owl:Class ;
    rdfs:subClassOf paizo:Event ;
    rdfs:label "Приключение"@ru,
                 "Adventure"@en ;
    rdfs:comment "Основное событие приключения. Включает в себя квесты, столкновения и другие события."@ru,
                 "Main adventure event. Includes quests, encounters and other events."@en .

paizo:Encounter rdf:type owl:Class ;
    rdfs:subClassOf paizo:Event ;
    rdfs:label "Столкновение"@ru,
                 "Encounter"@en ;
    rdfs:comment "Отдельное событие в приключении, предполагающее взаимодействие персонажей игроков с внешним миром."@ru,
                 "Individual event within an adventure, involving interaction between player characters and the external world."@en .

paizo:RandomEncounter rdf:type owl:Class ;
    rdfs:subClassOf paizo:Encounter ;
    rdfs:label "Случайное событие"@ru,
                 "Random Encounter"@en ;
    rdfs:comment "Случайное событие, которое может произойти во время путешествия."@ru,
                 "Random event that may occur during travel."@en .

paizo:Quest rdf:type owl:Class ;
    rdfs:subClassOf paizo:Event ;
    rdfs:label "Квест"@ru,
                 "Quest"@en ;
    rdfs:comment "Задача или миссия в приключении."@ru,
                 "Task or mission within an adventure."@en .

paizo:BackgroundEvent rdf:type owl:Class ;
    rdfs:subClassOf paizo:Event ;
    rdfs:label "Фоновое событие"@ru,
                 "Background Event"@en ;
    rdfs:comment "Событие, которое происходит вне влияния и контроля персонажей игроков."@ru,
                 "Event that occurs beyond the influence and control of player characters."@en .

paizo:Generation rdf:type owl:Class ;
    rdfs:subClassOf paizo:Event ;
    rdfs:label "Генерация"@ru,
                 "Generation"@en ;
    rdfs:comment "Генерация предмета или информации."@ru,
                 "Generation of an item or information."@en .

paizo:Consequence rdf:type owl:Class ;
    rdfs:subClassOf paizo:Event ;
    rdfs:label "Последствие"@ru,
                 "Consequence"@en ;
    rdfs:comment "Событие, являющееся результатом или последствием других событий или действий персонажей."@ru,
                 "Event that is a result or consequence of other events or character actions."@en .

# Классы для мест
paizo:Entity rdf:type owl:Class ;
    rdfs:label "Сущность"@ru,
                 "Entity"@en ;
    rdfs:comment "Сущность (объект), являющаяся объектом взаимодействия в игровом мире."@ru,
                 "Entity (object) that is an object of interaction in the game world."@en .

paizo:Location rdf:type owl:Class ;
    rdfs:subClassOf paizo:Entity ;
    rdfs:label "Локация"@ru,
                 "Location"@en ;
    rdfs:comment "Место действия в приключении."@ru,
                 "Place where adventure takes place."@en .

paizo:Settlement rdf:type owl:Class ;
    rdfs:subClassOf paizo:Location ;
    rdfs:label "Населенный пункт"@ru,
