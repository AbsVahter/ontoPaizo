    rdfs:comment "Торговое заведение."@ru,
                 "Commercial establishment."@en .

paizo:Room rdf:type owl:Class ;
    rdfs:subClassOf paizo:Location ;
    rdfs:label "Комната"@ru,
                 "Room"@en ;
    rdfs:comment "Отдельное помещение внутри здания или подземелья."@ru,
                 "Separate chamber within a building or dungeon."@en .

